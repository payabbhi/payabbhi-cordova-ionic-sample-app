<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PayabbhiDemo</name>
    <description>Sample Ionic app using Payabbhi Cordova Plugin</description>
    <author email="support@payabbhi.com" href="https://payabbhi.com/">Payabbhi</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="resources/android/icon/icon-mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/icon-hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/icon-xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/icon-xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/icon-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <!-- App Icons -->
        <!-- iPhone-->
        <icon height="180" width="180" src="resources/ios/icon/icon-60@3x.png"/>
        <icon height="120" width="120" src="resources/ios/icon/icon-60@2x.png"/>

        <!-- iPad Pro	167px × 167px (83.5pt × 83.5pt @2x)-->
        <icon height="167" width="167" src="resources/ios/icon/icon-83.5@2x.png"/>

        <!-- iPad, iPad mini	152px × 152px (76pt × 76pt @2x) -->
        <icon height="152" width="152" src="resources/ios/icon/icon-76@2x.png"/>

        <!-- App Store	1024px × 1024px (1024pt × 1024pt @1x) -->
        <icon height="1024" width="1024" src="resources/ios/icon/icon-1024.png"/>

        <!-- Spotlight Icons -->
        <!--iPhone	120px × 120px (40pt × 40pt @3x) -->
        <icon height="120" width="120" src="resources/ios/icon/icon-40@3x.png"/>

        <!--iPhone	80px × 80px (40pt × 40pt @2x) -->
        <!-- iPad Pro, iPad, iPad mini	80px × 80px (40pt × 40pt @2x) -->
        <icon height="80" width="80" src="resources/ios/icon/icon-40@2x.png"/>


        <!-- Settings icons -->
        <!--iPhone	87px × 87px (29pt × 29pt @3x) -->
        <icon height="87" width="87" src="resources/ios/icon/icon-29@3x.png"/>

        <!--iPhone	58px × 58px (29pt × 29pt @2x) -->
        <!--iPad Pro, iPad, iPad mini	58px × 58px (29pt × 29pt @2x) -->
        <icon height="58" width="58" src="resources/ios/icon/icon-29@2x.png"/>


        <!-- Notification icons-->
        <!-- iPhone	60px × 60px (20pt × 20pt @3x) -->
        <icon height="60" width="60" src="resources/ios/icon/icon-20@3x.png"/>
        <!-- iPhone	40px × 40px (20pt × 20pt @2x) -->
        <!-- iPad Pro, iPad, iPad mini	40px × 40px (20pt × 20pt @2x)-->
        <icon height="40" width="40" src="resources/ios/icon/icon-20@2x.png"/>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^2.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <engine name="browser" spec="~5.0.4" />
    <engine name="android" spec="~7.1.1" />
    <engine name="ios" spec="~4.5.5" />
</widget>
